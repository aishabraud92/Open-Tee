<h1 class="text-center">Confimred!!</h1>
<% if(results.length <= 0) { %>
  <h2>Nothing in the database yet</h2>
  <% } else { %>
    <% results.forEach(function(item){ %>
      <h3>
        <a href="/schedule/<%= item.id %>">
          <%= item.course %>
          <%= item.date %>
          <%= item.time %>
        </a>
      </h3>
      <a href="/event/profile" class="btn btn-danger delete-link">
        Back to profile
      </a>
    <% }); %>
  <% } %>
